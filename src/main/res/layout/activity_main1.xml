            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog12"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S12"
            android:textOn="S12"
            android:textSize="20dp"></ToggleButton>
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:baselineAligned="false">


        <ToggleButton
            android:id="@+id/tog13"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S13"
            android:textOn="S13"
            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog14"
            android:layout_width="0dp"
