<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout02"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="1dp"
    android:paddingTop="1dp"
    android:paddingRight="1dp"
    android:paddingBottom="1dp">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:baselineAligned="false">


        <ToggleButton
            android:id="@+id/tog1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S1"
            android:textOn="已选择"
            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S2"
            android:textOn="S2"
            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S3"
            android:textOn="S3"
            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog4"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S4"
            android:textOn="S4"
            android:textSize="20dp"></ToggleButton>
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">


        <ToggleButton
            android:id="@+id/tog5"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S5"
            android:textOn="S5"
            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog6"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S6"
            android:textOn="S6"
            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog7"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S7"
            android:textOn="S7"
            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog8"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S8"
            android:textOn="S8"
            android:textSize="20dp"></ToggleButton>
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:baselineAligned="false">


        <ToggleButton
            android:id="@+id/tog9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S9"
            android:textOn="S9"
            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog10"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textOff="S10"
            android:textOn="S10"
            android:textSize="20dp"></ToggleButton>

        <ToggleButton
            android:id="@+id/tog11"
            android:layout_width="0dp"
